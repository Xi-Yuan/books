<!DOCTYPE html>
<html class="um landscape min-width-240px min-width-320px min-width-480px min-width-768px min-width-1024px">
    <head>
        <title></title>
        <meta charset="utf-8">
        <meta name="viewport" content="target-densitydpi=device-dpi, width=device-width, initial-scale=1, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
        <link rel="stylesheet" href="css/fonts/font-awesome.min.css">
        <link rel="stylesheet" href="css/ui-box.css">
        <link rel="stylesheet" href="css/ui-base.css">
        <link rel="stylesheet" href="css/ui-color.css">
        <link rel="stylesheet" href="css/appcan.icon.css">
        <link rel="stylesheet" href="css/appcan.control.css">
        <link rel="stylesheet" href="book_info/css/main.css">
    </head>
    <body class="um-vp bc-bg" ontouchstart>
        <div id="page_0" class="up ub ub-ver" tabindex="0">
            <!--header开始-->
            <div id="header" class="uh bc-text-head ub bc-head">
                <div class="nav-btn" id="nav-left">
                    <div class="fa  fa-angle-left fa-2x"></div>
                </div>
                <h1 class="ut ub-f1 ulev-3 ut-s tx-c" tabindex="0">二手书</h1>
                <div class="nav-btn nav-bt" id="nav-right">
                    <div class="fa  fa-cog ulev2"></div>
                </div>
            </div>
            <!--header结束--><!--content开始-->
            <div id="content" class="ub-f1 tx-l t-bla c-wh">

            </div>
            <!--content结束-->
            <div id="footer" class="uf t-bla ub ubt b-gra4">
                <div class="ub ub-f1 tab" id="footer1">
                    <div class="pos_re ub ub-f1">
                        <input class="uhide" type="radio" name="tabSwitch" checked="true">
                        <div onmousedown="appcan.touch('',appcan.elementFor)" ontouchstart="appcan.touch('',appcan.elementFor)" class="ub-f1 ub ub-ver">
                            <div class="ub-f1 ub-img5 tp-home2"></div>
                            <div class="uinn-a13 ulev-1 tx-c act-col-blu">
                                首页
                            </div>
                        </div>
                    </div>
                    <div class="pos_re ub ub-f1">
                        <input class="uhide" type="radio" name="tabSwitch">
                        <div onmousedown="appcan.touch('',appcan.elementFor)" ontouchstart="appcan.touch('',appcan.elementFor)" class="ub-f1 ub ub-ver">
                            <div class="ub-f1 ub-img5 tp-sort2"></div>
                            <div class="uinn-a13 ulev-1 tx-c act-col-blu">
                                卖书
                            </div>
                        </div>
                    </div>
                    <div class="pos_re ub ub-f1">
                        <input class="uhide" type="radio" name="tabSwitch">
                        <div onmousedown="appcan.touch('',appcan.elementFor)" ontouchstart="appcan.touch('',appcan.elementFor)" class="ub-f1 ub ub-ver">
                            <div class="ub-f1 ub-img5 tp-my2"></div>
                            <div class="uinn-a13 ulev-1 tx-c act-col-blu">
                                我
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>
    <script src="js/appcan.js"></script>
    <script src="js/appcan.control.js"></script>
    <script src="js/appcan.tab.js"></script>
    </body>
    <script>
        appcan.ready(function() {
            var titHeight = $('#header').offset().height;
            appcan.frame.open("content", "book_info_content.html", 0, titHeight);
            window.onorientationchange = window.onresize = function() {
                appcan.frame.resize("content", 0, titHeight);
            }
        });
        var tabview = appcan.tab({
            selector : "#footer1",
            hasIcon : true,
            hasAnim : false,
            hasLabel : true,
            hasBadge : true,
            index : 3,
            data : [{
                label : "首页",
                icon : "fa-home"
            }, {
                label : "卖书",
                icon : "fa-list"
            }, {
                label : "我",
                icon : "fa-user"
            }]
        });
        appcan.button("#nav-left", "btn-act", function() {
            appcan.window.close();
        })
    </script>
</html>